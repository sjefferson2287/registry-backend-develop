{
  "name": "aws-node-express-api-redgistry",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "nodemon app.js",
    "dev": "nodemon --exec serverless offline start --stage local -e js yml json",
    "test": "nyc mocha --timeout=5000 --exit",
    "init-githooks": "cd .. && git config core.hooksPath ./git-hooks",
    "lint-js": "eslint --ext .js src/ && eslint --ext .js database/",
    "lint-js:fix": "eslint --ext .js src/ --fix && eslint --ext .js database/ --fix",
    "create-model": "npx sequelize-cli model:generate --name $MODEL --attributes firstName:string",
    "create-migration": "npx sequelize-cli migration:create --name $NAME",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:down": "npx sequelize-cli db:migrate:undo",
    "migrate:one:up": "npx sequelize-cli db:migrate --to 20230419000000-create-academic_details.js",
    "migrate:one:down": "npx sequelize db:migrate:undo --name 20230419000000-create-academic_details.js"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@jsdevtools/ono": "^7.1.3",
    "aws-lambda-multipart-parser": "^0.1.3",
    "aws-sdk": "^2.1200.0",
    "axios": "^0.27.2",
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "https": "^1.0.0",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "nodemon": "^2.0.19",
    "path": "^0.12.7",
    "pg": "^8.7.3",
    "pg-format": "^1.0.4",
    "prettier": "^2.7.1",
    "qa": "^0.0.1",
    "querystring": "^0.2.1",
    "request": "^2.88.2",
    "sequelize": "^6.31.0",
    "sequelize-cli": "^6.6.0",
    "serverless-api-compression": "^1.0.1",
    "serverless-middleware": "^3.1.0",
    "serverless-offline": "^9.2.6",
    "twilio": "^3.84.1",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3"
  }
}
